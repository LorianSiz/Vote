pipeline {
    agent any
    
	stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh "cd vote"
                sh "mvn build"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}